import {ao_latest} from 'roap'

import @{}
  assert, expect
  expect_ao_latest, expect_ao_api
from './_utils.jsy'

describe @ 'core_latest', @::
  it @ 'smoke', @::
    expect(ao_latest).to.be.a('function')

  it @ 'shape', @::
    const res = ao_latest()
    expect(res).to.be.an('array').of.length(2)

    const [aod, ao_api] = res
    expect_ao_latest(aod)
    expect_ao_api(ao_api)

